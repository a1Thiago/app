{
  "extends": "next/core-web-vitals",
  "plugins": [
    "import",
    "@typescript-eslint"
  ],
  "rules": {
    "semi": [
      "warn",
      "never"
    ],
    "quotes": [
      "warn",
      "single"
    ],
    "no-unused-vars": "warn",
    "no-unused-expressions": "warn",
    "no-unused-imports": "warn"
  }
}